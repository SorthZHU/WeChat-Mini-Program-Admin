<!-- 顶部导航栏 -->
<view class="pubTitle">
      <view class="txt">通知管理</view>
      <navigator url="" class="more" open-type="reLaunch" bindtap="jumpToAdd">
        <image src="cloud://wxyun-7g4bm1vn16ceb7ed.7778-wxyun-7g4bm1vn16ceb7ed-1304852033/icon/加.png"></image>
      </navigator>
</view>

<view class="container">
<view wx:for="list">
  <l-list title="购物车" />
</view>

  
  <!-- 查询内容 这里面定义实体类里面的字段-->
  <view class="row">
    <block wx:for="{{list}}" wx:key="key">
      <view class="content">
        <view class="title" >
          标题:{{item.title}}
        </view>
        <view >
          作者:{{item.author}}
        </view>
        

        <view class="opration">
          <image src="cloud://wxyun-7g4bm1vn16ceb7ed.7778-wxyun-7g4bm1vn16ceb7ed-1304852033/icon/删 除 .png" class="searchicon" data-itemid='{{item._id}}' bindtap="onDel" />
           
          <navigator url="../editNotice/editNotice?itemid={{item._id}}"  class="edit">
            <image src="cloud://wxyun-7g4bm1vn16ceb7ed.7778-wxyun-7g4bm1vn16ceb7ed-1304852033/icon/编辑.png" class="searchicon" data-itemid='{{item._id}}' />
            
          </navigator>
        </view>
      </view>
    </block>
    <view wx:if='{{noData}}'class="noData">没有更多了~</view>
  </view>
</view>